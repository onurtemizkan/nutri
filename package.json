{
  "name": "nutri",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:device": "npx expo run:ios --device",
    "web": "expo start --web",
    "reset-project": "node ./scripts/reset-project.js",
    "test": "jest",
    "lint": "expo lint",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"app/**/*.{js,jsx,ts,tsx}\" \"lib/**/*.{js,jsx,ts,tsx}\" \"hooks/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"app/**/*.{js,jsx,ts,tsx}\" \"lib/**/*.{js,jsx,ts,tsx}\" \"hooks/**/*.{js,jsx,ts,tsx}\"",
    "docker:dev": "./scripts/docker-dev.sh",
    "docker:dev:start": "./scripts/docker-dev.sh start",
    "docker:dev:stop": "./scripts/docker-dev.sh stop",
    "docker:dev:logs": "./scripts/docker-dev.sh logs",
    "docker:dev:build": "./scripts/docker-dev.sh build",
    "docker:dev:clean": "./scripts/docker-dev.sh clean",
    "docker:dev:status": "./scripts/docker-dev.sh status",
    "docker:dev:shell:backend": "docker exec -it nutri-dev-backend sh",
    "docker:dev:shell:ml": "docker exec -it nutri-dev-ml-service bash",
    "dev:services": "./scripts/start-dev.sh",
    "dev:services:stop": "./scripts/stop-dev.sh",
    "dev:start": "./scripts/start-all.sh",
    "dev:stop": "./scripts/stop-all.sh",
    "dev:full": "./scripts/start-full.sh",
    "dev:backend": "./scripts/start-backend.sh",
    "test:e2e": "./e2e/scripts/run-e2e-tests.sh",
    "test:e2e:seed": "./e2e/scripts/run-e2e-tests.sh --seed",
    "test:e2e:parallel": "./e2e/scripts/run-e2e-tests.sh --parallel",
    "test:e2e:suites": "cd e2e && ./run_suites.sh",
    "test:e2e:suite:main": "cd e2e && maestro test tests/suites/main_suite.yaml",
    "test:e2e:suite:profile": "cd e2e && maestro test tests/suites/profile_suite.yaml",
    "test:e2e:suite:scan": "cd e2e && maestro test tests/suites/scan_suite.yaml",
    "test:e2e:suite:auth": "cd e2e && maestro test tests/suites/auth_suite.yaml",
    "test:e2e:auth": "./e2e/scripts/run-e2e-tests.sh auth",
    "test:e2e:meals": "./e2e/scripts/run-e2e-tests.sh meals",
    "test:e2e:health": "./e2e/scripts/run-e2e-tests.sh health",
    "test:e2e:profile": "./e2e/scripts/run-e2e-tests.sh profile",
    "test:e2e:supplements": "./e2e/scripts/run-e2e-tests.sh supplements",
    "deploy:device": "./scripts/deploy-device.sh",
    "deploy:device:local": "./scripts/deploy-device.sh --local",
    "deploy:device:clean": "./scripts/deploy-device.sh --local --clean",
    "build:ios": "eas build --profile development --platform ios",
    "build:ios:local": "eas build --profile development-local --platform ios --local",
    "build:ios:simulator": "eas build --profile simulator --platform ios --local",
    "build:ios:xcode": "eas build --profile development-xcode --platform ios --local",
    "prebuild:ios": "npx expo prebuild --platform ios",
    "prebuild:ios:clean": "npx expo prebuild --platform ios --clean",
    "deploy:native": "./scripts/deploy-native.sh",
    "deploy:native:debug": "./scripts/deploy-native.sh --debug",
    "deploy:native:clean": "./scripts/deploy-native.sh --clean",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@gorhom/portal": "^1.0.14",
    "@kingstinct/react-native-healthkit": "^13.0.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "^11.0.0",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@types/base-64": "^1.0.2",
    "axios": "^1.7.9",
    "base-64": "^1.0.0",
    "expo": "~54.0.29",
    "expo-apple-authentication": "~8.0.8",
    "expo-blur": "~15.0.8",
    "expo-camera": "~17.0.10",
    "expo-constants": "~18.0.12",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-gl": "~16.0.9",
    "expo-haptics": "~15.0.8",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.10",
    "expo-notifications": "~0.32.15",
    "expo-router": "~6.0.19",
    "expo-screen-orientation": "~9.0.8",
    "expo-secure-store": "~15.0.8",
    "expo-sensors": "~15.0.8",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "jest-expo": "~54.0.16",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-nitro-modules": "^0.31.10",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "task-master-ai": "^0.37.0",
    "three": "^0.170.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~10.0.0",
    "husky": "^9.1.7",
    "ios-simulator-mcp": "^1.5.1",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.16",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "@expo/fingerprint": "^0.15.4"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-health",
          "lidar-module"
        ]
      }
    }
  },
  "private": true
}
